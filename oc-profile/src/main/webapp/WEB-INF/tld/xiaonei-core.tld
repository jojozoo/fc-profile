<?xml version="1.0" encoding="UTF-8"?>
<!--CTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN" </sp-->
<taglib>
    <tlibversion>1.2</tlibversion>
    <jspversion>1.1</jspversion>
    <shortname>tags-core</shortname>
    <uri>http://www.xiaonei.com/tags-core</uri>
    <tag>
        <name>footer</name>
        <tagclass>com.xiaonei.platform.taglib.menu.FooterTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>icp</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>netpolice</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>sidebar</name>
        <tagclass>com.xiaonei.platform.taglib.menu.SidebarTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>type</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>navigation</name>
        <tagclass>com.xiaonei.platform.taglib.menu.NavigationTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>imnotice</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>head</name>
        <tagclass>com.xiaonei.platform.taglib.menu.HeadTag</tagclass>
        <bodycontent>JSP</bodycontent>
    </tag>
    <tag>
        <name>networkName</name>
        <tagclass>com.xiaonei.platform.taglib.name.NetworkNameTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>networkId</name>
            <required>true</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>userMainUrl</name>
        <tagclass>com.xiaonei.platform.taglib.url.UserMainUrlTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>url</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>userTinyUrl</name>
        <tagclass>com.xiaonei.platform.taglib.url.UserTinyUrlTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>url</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>userHeadUrl</name>
        <tagclass>com.xiaonei.platform.taglib.url.UserHeadUrlTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>url</name>
            <required>true</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>sysMsg</name>
        <tagclass>com.xiaonei.platform.taglib.sysmsg.SysMsgTag</tagclass>
        <bodycontent>empty</bodycontent>
    </tag>
</taglib>